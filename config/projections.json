{
  "Gemfile": {
    "command": "gemfile"
  }, 
  "config/routes.rb": {
    "command": "routes"
  },
  "config/projections.json": {
    "command": "projections"
  }, 
  "spec/factories.rb": {
    "command": "factories"
  },
  "lib/tasks/populate.rake": {
    "command": "populate"
  },
  "spec/features/*_spec.rb": {
    "command": "rfeature"
  },
  "spec/models/*_spec.rb": {
    "command": "rmodel"
  },
  "spec/requests/*_spec.rb": {
    "command": "rrequest"
  }
}
